{
  "name": "asuna-admin",
  "version": "0.5.0",
  "repository": "git@github.com:danielwii/asuna-admin.git",
  "author": "Daniel Wei <danielwii0326@gmail.com>",
  "license": "MIT",
  "bin": {
    "asuna": "bin/asuna.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "jest": {
    "globals": {
      "ts-jest": {
        "babelConfig": {
          "plugins": [
            [
              "babel-plugin-async-import"
            ]
          ]
        },
        "tsConfigFile": "tsconfig.jest.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/.asuna"
    ],
    "moduleDirectories": [
      "node_modules",
      "."
    ],
    "modulePathIgnorePatterns": [
      "/.asuna"
    ],
    "testRegex": ".spec.tsx?$",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "coverageThreshold": {
      "global": {}
    },
    "collectCoverage": true
  },
  "scripts": {
    "autod": "autod",
    "dev": "NODE_ENV=dev node $NODE_DEBUG_OPTION server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "storybook": "NODE_ENV=dev start-storybook -p 6006",
    "build-storybook": "NODE_ENV=dev build-storybook",
    "collect": "sh ./bin/collect.sh",
    "pre-commit": "jest",
    "test": "ENV=test jest --no-cache",
    "changelog": "conventional-changelog -p asuna-admin -i CHANGELOG.md",
    "init-changelog": "conventional-changelog -p asuna-admin -i CHANGELOG.md -r 0",
    "postinstall": "yarn collect"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "~0.0.19",
    "@zeit/next-typescript": "^1.0.0",
    "antd": "^3.5.0",
    "async": "^2.6.0",
    "axios": "^0.18.0",
    "braft-editor": "~1.9.1",
    "colors": "~1.2.4",
    "commander": "^2.15.1",
    "cross-spawn": "^6.0.5",
    "css-modules-require-hook": "^4.2.3",
    "debug": "^3.1.0",
    "deep-diff": "^1.0.0",
    "draft-js": "^0.10.5",
    "http-proxy": "^1.17.0",
    "immutable": "^3.8.2",
    "localforage": "^1.7.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "ncp": "^2.0.0",
    "next": "~6.0.1",
    "next-progressbar": "^0.0.2",
    "next-redux-saga": "^1",
    "next-redux-wrapper": "^1.3.5",
    "node-buffs": "~0.5.1",
    "object-hash": "^1.3.0",
    "polished": "^1.9.2",
    "postcss-import": "^11.1.0",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-animations": "^1.0.0",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-sortable-hoc": "^0.6.8",
    "react-truncate": "^2.3.0",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.18.0",
    "redux-persist": "~4",
    "redux-saga": "^0.16.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.10",
    "shelljs": "^0.8.2",
    "socket.io-client": "^2.1.0",
    "styled-components": "^3.2.6",
    "typescript": "^2.8.3",
    "video.js": "^6.9.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.3",
    "@storybook/addon-links": "^3.4.3",
    "@storybook/react": "^3.4.3",
    "@types/braft-editor": "^1.1.2",
    "@types/debug": "^0.0.30",
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^22.2.3",
    "@types/next-redux-wrapper": "^1.4.1",
    "@types/prop-types": "^15.5.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/styled-jsx": "^2.2.3",
    "@types/video.js": "^6.2.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-async-import": "^2.0.1",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.15.0-rc.8",
    "jest": "^22.4.3",
    "ts-jest": "joaovieira/ts-jest#babel-peerDependency-release",
    "tsconfig-paths": "^3.3.2",
    "tsconfig-paths-webpack-plugin": "^3.0.4",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-jarvis": "0.3.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
