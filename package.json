{
  "private": true,
  "name": "asuna-admin-workspace",
  "version": "0.0.0",
  "workspaces": [
    "core",
    "!examples/*"
  ],
  "scripts": {
    "test": "yarn workspace asuna-admin test",
    "clean": "yarn workspace asuna-admin clean",
    "build": "yarn workspace asuna-admin build",
    "outdated": "yarn workspaces run outdated"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^9.1.2",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "commitlint": "^9.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.8.1",
    "eslint-config-alloy": "^3.8.0",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.0.0",
    "typescript": "^4.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
