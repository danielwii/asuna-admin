{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2020",
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "jsx": "preserve",
    "allowJs": false,
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "tsBuildInfoFile": "./.buildinfo.json",
    "declaration": true,
    "outDir": "dist",
    "baseUrl": "./src",
    "paths": {
      "asuna-admin": ["index.ts"],
      "@asuna-admin/*": ["*"]
    },
    "noEmit": true
  },
  "include": ["next-env.d.ts", "src/**/*.ts*", "./services/**/*.ts*", "./components/**/*.ts*", "./pages/**/*.ts*"],
  "exclude": ["**/*.spec.*", "node_modules"]
}
