{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "esnext",
    "target": "es2020",
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "jsx": "preserve",
    "allowJs": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "declaration": true,
    "outDir": "dist",
    "baseUrl": "./src",
    "paths": {
      "asuna-admin": [
        "index.ts"
      ],
      "@asuna-admin/*": [
        "*"
      ]
    },
    "noEmit": true
  },
  "include": [
    "next-env.d.ts",
    "src/**/*.ts*",
    "./services/**/*.ts*",
    "./components/**/*.ts*",
    "./pages/**/*.ts*"
  ],
  "exclude": [
    "**/*.spec.*",
    "node_modules"
  ]
}
