{
  "name": "asuna-admin",
  "version": "0.12.2",
  "repository": "git@github.com:danielwii/asuna-admin.git",
  "author": "Daniel Wei <danielwii0326@gmail.com>",
  "license": "MIT",
  "files": [
    "dist",
    "typings",
    "server",
    "asuna-babel.js",
    "asuna-default-http-server.js",
    "asuna-default-koa-server.js",
    "asuna-with-default-next.js",
    "asuna-utils.js"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "declaration/index.d.ts",
  "scripts": {
    "autod": "autod",
    "dev": "NODE_ENV=dev node $NODE_DEBUG_OPTION server.js",
    "clean": "rm -rf ./dist ./declaration",
    "prebuild-ignore": "yarn clean",
    "build": "tsc --noEmit false --declaration true --declarationDir declaration --isolatedModules false",
    "postinstall-ignore": "yarn build",
    "postbuild": "if [[ -e ./dist/src ]]; then mv ./dist ./_dist && mv ./_dist/src ./dist && rm -rf ./_dist; fi && if [[ -e ./declaration/src ]]; then mv ./declaration ./_declaration && mv ./_declaration/src ./declaration && rm -rf ./_declaration; fi && yarn compile:replace-path",
    "postpublish": "yarn clean",
    "compile:replace-path": "node replace-path.js",
    "start": "NODE_ENV=production node server.js",
    "storybook": "NODE_ENV=dev start-storybook -p 6006",
    "build-storybook": "NODE_ENV=dev build-storybook",
    "pre-commit": "jest",
    "test": "ENV=test jest",
    "changelog": "conventional-changelog -p asuna-admin -i CHANGELOG.md",
    "init-changelog": "conventional-changelog -p asuna-admin -i CHANGELOG.md -r 0",
    "check:circular": "npx madge --circular --extensions ts ./"
  },
  "peerDependencies": {},
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "@zeit/next-css": "1.0.1",
    "antd": "3.20.3",
    "apollo-boost": "^0.4.3",
    "apollo-server-koa": "^2.7.0",
    "axios": "0.19.0",
    "bluebird": "3.5.5",
    "braft-editor": "2.3.7",
    "colors": "^1.3.3",
    "debug": "4.1.1",
    "deep-diff": "1.0.2",
    "graphql": "^14.4.2",
    "http-proxy": "1.17.0",
    "idx": "^2.5.6",
    "immutable": "3.8.2",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "localforage": "1.7.3",
    "lodash": "4.17.15",
    "lru-cache": "^5.1.1",
    "merge-graphql-schemas": "^1.5.8",
    "moment": "2.24.0",
    "next": "9.0.2",
    "next-redux-wrapper": "3.0.0-alpha.3",
    "next-transpile-modules": "^2.3.1",
    "node-buffs": "^0.6.4",
    "object-hash": "1.3.1",
    "ramda": "0.26.1",
    "react": "16.8.6",
    "react-animations": "1.0.0",
    "react-apollo": "^2.5.8",
    "react-apollo-hooks": "^0.5.0",
    "react-dom": "16.8.6",
    "react-highlight": "^0.12.0",
    "react-image-crop": "^7.0.5",
    "react-pdf": "^4.1.0",
    "react-redux": "7.1.0",
    "react-sortable-hoc": "1.9.1",
    "react-spinners": "^0.5.12",
    "react-truncate": "2.4.0",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-logger": "3.0.6",
    "redux-observable": "1.1.0",
    "redux-persist": "5.10.0",
    "redux-saga": "1.0.5",
    "rxjs": "6.5.2",
    "socket.io-client": "^2.2.0",
    "stream-array": "^1.1.2",
    "styled-components": "4.3.2",
    "url-join": "4.0.1",
    "video.js": "^7.6.0"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.5.0",
    "@babel/plugin-transform-runtime": "7.5.0",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-info": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@types/bluebird": "3.5.27",
    "@types/debug": "4.1.4",
    "@types/jest": "^24.0.15",
    "@types/lodash": "4.14.136",
    "@types/next-redux-wrapper": "2.0.2",
    "@types/react-redux": "^7.1.1",
    "@types/socket.io-client": "1.4.32",
    "@types/socket.io-redis": "^1.0.25",
    "@types/storybook__react": "^4.0.2",
    "@types/styled-components": "^4.1.18",
    "@types/styled-jsx": "2.2.8",
    "@types/url-join": "4.0.0",
    "@types/video.js": "^7.2.10",
    "@types/webpack": "^4.4.35",
    "babel-loader": "8.0.6",
    "babel-plugin-async-import": "2.1.0",
    "babel-plugin-idx": "^2.4.0",
    "babel-plugin-import": "1.12.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-styled-components": "1.10.6",
    "eslint-plugin-react-hooks": "^1.6.1",
    "glob": "7.1.4",
    "jest": "^24.8.0",
    "source-map-loader": "0.2.4",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.4",
    "ts-simple-ast": "21.0.4",
    "tsconfig-extends": "1.0.1",
    "tsconfig-paths": "3.8.0",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.6",
    "webpack-node-externals": "1.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
